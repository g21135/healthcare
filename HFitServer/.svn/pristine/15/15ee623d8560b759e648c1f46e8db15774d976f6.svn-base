package kr.or.ddit.vo;

import java.io.Serializable;

public class ChallApplyVO implements Serializable{
	
	private int challapply_no;
	private String mem_id ;
	private int chall_no;
	
	public ChallApplyVO(int challapply_no, String mem_id, int chall_no) {
		super();
		this.challapply_no = challapply_no;
		this.mem_id = mem_id;
		this.chall_no = chall_no;
	}
	public ChallApplyVO() {
		
	}
	public int getChallapply_no() {
		return challapply_no;
	}
	public void setChallapply_no(int challapply_no) {
		this.challapply_no = challapply_no;
	}
	public String getMem_id() {
		return mem_id;
	}
	public void setMem_id(String mem_id) {
		this.mem_id = mem_id;
	}
	public int getChall_no() {
		return chall_no;
	}
	public void setChall_no(int chall_no) {
		this.chall_no = chall_no;
	}
	
	

}
