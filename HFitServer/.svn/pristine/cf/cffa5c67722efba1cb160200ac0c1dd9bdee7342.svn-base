<sqlMap namespace="diary">
	<insert id="insert" parameterClass="diaryVO">
	insert into diaryvo(id,mem_id, title,
	fullDay,startDate,endDate,startTime,endTime,zoneId,recurring,rrule,recurrence,entryType)
	 values(#id#,#mem_id#,#title#,#fullDay#,#startDate#,#endDate#,#startTime#,#endTime#,#zoneId#,#recurring#,#rrule#,#recurrence#,#entryType#)
	</insert>
	
	<delete id="delete" parameterClass="int">
	delete from diaryvo where id = #id#
	</delete>
	
	<update id="update" parameterClass="diaryVO">
	update diaryvo set 
	mem_id = #mem_id#, 
	title = #title#, 
	fullDay = #fullDay#, 
	startDate = #startDate#, 
	endDate = #endDate#,
	startTime = #startTime#,
	endTime = #endTime#,
	zoneId = #zoneId#,
	recurring = #recurring#,
	recurrence = #recurrence#, 
	entryType = #entryType# where id = #id# 
