<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
			PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
			"http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="apply">
	
	<insert id="insert" parameterClass="applyVO">
		insert into ApplyVO values(apply_seq.nextval, #apply_name#, #apply_bir#, #apply_addr#, 
						#apply_tel#, #apply_careeri#, #apply_gender#, #apply_email#)
	</insert>
	
	<select id="getAllApplyList" resultClass="applyVO"> 
		select * from ApplyVO
	</select>
	
	<delete id="deleteApply" parameterClass="int">
		delete from applyvo where apply_no = #apply_no# 
	</delete>

</sqlMap>