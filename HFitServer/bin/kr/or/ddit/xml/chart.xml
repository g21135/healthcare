<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap
			PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
			"http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="chart">
	<select id="getAllChart" resultClass="chartVO"
		parameterClass="chartVO">
		select mem_id, mem_kg, mem_height, chart_date from
		chartvo
		where mem_id=#mem_id# order by 4

	</select>
	<insert id="insert" parameterClass="chartVO" >
		insert into chartvo values(#mem_id#, #mem_kg#, #mem_height#, #chart_date#)
	</insert>
</sqlMap>