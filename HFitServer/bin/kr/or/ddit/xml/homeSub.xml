<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap
			PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
			"http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="homeSub">
	<insert id="insert" parameterClass="homeSubVO" >
	insert into homeSubVO values(HOMESUBSEQUENCE.nextval, #mem_id#, #home_no#)
	</insert>
	
	<delete id="delete" parameterClass="homeSubVO" >
	delete homeSubVO where mem_id = #mem_id# and home_no = #home_no#
	</delete>
	
	<select id="selectAll" resultClass="homeSubVO" >
	select * from homeSubVO
	</select>
</sqlMap>