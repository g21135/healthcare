<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="550.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kr.or.ddit.view.admin.trainerMainController">
   <children>
      <TableView fx:id="trainerTable" layoutX="55.0" layoutY="21.0" prefHeight="461.0" prefWidth="1191.0">
        <columns>
          <TableColumn fx:id="num" maxWidth="1500.0" prefWidth="75.0" text="번호" />
            <TableColumn fx:id="id" maxWidth="4000.0" prefWidth="75.0" text="아이디" />
            <TableColumn fx:id="name" maxWidth="3000.0" prefWidth="75.0" text="트레이너명" />
            <TableColumn fx:id="gender" maxWidth="2000.0" prefWidth="75.0" text="성별" />
            <TableColumn fx:id="age" maxWidth="3000.0" prefWidth="75.0" text="나이" />
            <TableColumn fx:id="specialty" maxWidth="2000.0" prefWidth="75.0" text="전문분야" />
          <TableColumn fx:id="check" maxWidth="2000.0" prefWidth="75.0" text="선택" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <JFXButton layoutX="563.0" layoutY="499.0" onAction="#updateBtnClick" prefHeight="35.0" prefWidth="64.0" style="-fx-background-color: pink;" text="수정" />
      <JFXButton layoutX="706.0" layoutY="499.0" onAction="#deleteBtnClick" prefHeight="35.0" prefWidth="64.0" style="-fx-background-color: pink;" text="삭제" />
      <JFXButton layoutX="55.0" layoutY="499.0" onAction="#applyList" prefHeight="35.0" prefWidth="171.0" style="-fx-background-color: pink;" text="등록신청 현황 보기" />
   </children>
</AnchorPane>
