          <TableColumn fx:id="num" maxWidth="1500.0" prefWidth="75.0" text="번호" />
            <TableColumn fx:id="name" maxWidth="3000.0" prefWidth="75.0" text="신청명" />
            <TableColumn fx:id="bir" maxWidth="2000.0" prefWidth="75.0" text="생년월일" />
            <TableColumn fx:id="addr" maxWidth="3000.0" prefWidth="75.0" text="주소" />
            <TableColumn fx:id="tel" maxWidth="3000.0" prefWidth="75.0" text="번호" />
            <TableColumn fx:id="career" maxWidth="2000.0" prefWidth="75.0" text="경력" />
            <TableColumn fx:id="gender" maxWidth="2000.0" prefWidth="75.0" text="성별" />
          <TableColumn fx:id="check" maxWidth="2000.0" prefWidth="75.0" text="선택" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <JFXButton layoutX="563.0" layoutY="499.0" onAction="#yesBtnClick" prefHeight="35.0" prefWidth="64.0" style="-fx-background-color: pink;" text="승인" />
      <JFXButton layoutX="706.0" layoutY="499.0" onAction="#noBtnClick" prefHeight="35.0" prefWidth="64.0" style="-fx-background-color: pink;" text="거절" />
